{
  "Authentication": {
    "AzureAD": {
      "AadInstance": "https://login.microsoftonline.com/{0}",
      "Tenant": "{your tenant name}", /* e.g. contoso.onmicrosoft.com*/
      "Audience": "{Your APP ID URI}" /* e.g. http://contoso.onmicrosoft.com/InventoryService*/
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
