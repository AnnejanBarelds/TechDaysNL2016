{
  "Authentication": {
    "AzureAD": {
      "AadInstance": "https://login.microsoftonline.com/{0}",
      "Tenant": "{your tenant name}", /* e.g. contoso.onmicrosoft.com*/
      "ClientId": "{your Client ID}",
      "ClientSecret": "{Your Key}",
      "InventoryServiceResourceId": "{The APP ID Uri of a resource}"/* e.g. "http://contoso.onmicrosoft.com/InventoryService" */,
      "Audience": "{Your APP ID URI}" /* e.g. http://contoso.onmicrosoft.com/OrderService*/
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
