{
  "Authentication": {
    "AzureAd": {
      "AadInstance": "https://login.microsoftonline.com/{0}",
      "CallbackPath": "/signin-oidc",
      "ClientId": "{your Client ID}",
      "ClientSecret": "{Your Key}",
      "Tenant": "{your tenant name}", /* e.g. contoso.onmicrosoft.com*/
      "TenantId": "{your tenant id}",
      "OrderServiceResourceId": "{The APP ID Uri of a resource}" /* e.g. "http://contoso.onmicrosoft.com/OrderService" */,
      "InventoryServiceResourceId": "{The APP ID Uri of a resource}" /* e.g. "http://contoso.onmicrosoft.com/InventoryService" */,
      "GraphResourceId": "https://graph.windows.net"
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}